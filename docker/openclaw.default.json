{
  "server": {
    "port": 18789,
    "host": "0.0.0.0"
  },
  "browser": {
    "enabled": true,
    "defaultProfile": "browserless",
    "profiles": {
      "browserless": {
        "type": "cdp",
        "cdpUrl": "${BROWSERLESS_URL}"
      }
    }
  },
  "memory": {
    "enabled": true,
    "maxItems": 2000
  },
  "hooks": {
    "onMessage": {
      "url": "${PLATFORM_API}/webhooks/container/message",
      "headers": {
        "x-internal-secret": "${INTERNAL_SECRET}"
      }
    }
  }
}
